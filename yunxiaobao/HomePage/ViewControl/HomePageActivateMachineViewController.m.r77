@implementation HomePageActivateMachineViewController

- (void)viewDidLoad {
    [super viewDidLoad];
    self.titleString = @"激活机具";
    self.tableView = [[UITableView alloc] initWithFrame:CGRectZero style:UITableViewStylePlain];
    self.tableView.frame = CGRectMake(0, DelouchStatusBarAndNavigationBarHeight, DelouchWidth, DelouchHeight - DelouchStatusBarAndNavigationBarHeight - DelouchTabbarSafeBottomMargin - (49 * self.baseicFloat));
    self.tableView.delegate = self;
    self.tableView.dataSource = self;
    [self.view addSubview:self.tableView];
    UIView *footView = [[UIView alloc] initWithFrame:CGRectMake(0, 0, DelouchWidth, 50)];
    UIButton *nextStepBtn = [UIButton buttonWithType:UIButtonTypeCustom];
    [nextStepBtn setTitle:@"下一步" forState:UIControlStateNormal];
    //[nextStepBtn setTitleColor:[UIColor blueColor] forState:UIControlStateNormal];
    [nextStepBtn setBackgroundColor:[UIColor blueColor]];
    [nextStepBtn setFrame:CGRectMake(30, 10, DelouchWidth - 60, 30)];
    [nextStepBtn addTarget:self action:@selector(nextStep) forControlEvents:UIControlEventTouchUpInside];
    [footView addSubview:nextStepBtn];
    self.tableView.tableFooterView = footView;
    pickerArray = [NSMutableArray array];
   
}


-(void)addPickerView{
    
    myPickerView = [[UIPickerView alloc]init];
    myPickerView.frame = CGRectMake(0, DelouchHeight - 100, DelouchWidth, 100);
    myPickerView.dataSource = self;
    myPickerView.delegate = self;
    myPickerView.showsSelectionIndicator = YES;
    myPickerView.backgroundColor = [UIColor lightGrayColor];
    [self.view addSubview:myPickerView];
    
}


-(void)nextStep{
    
  //[self pushViewControl:@"UpdateCertificateViewController" propertyDic:nil];
    [self getParameter];
    
    
}

-(void)getParameter{
    
    
    if (self.snTextField.text.length == 0 || self.merchantTextField.text.length == 0 || self.merchantNameTextField.text.length == 0 || self.merchantPhoneTextField.text.length == 0 || self.idCardTextField.text.length == 0 || self.bankNumberTextField.text.length == 0 || self.bankNameTextField.text.length == 0 || self.bandAddrLabel.text.length == 0 || self.bankInternetLabel.text.length == 0 || self.phoneTextField.text.length == 0 || self.posAddrLabel.text.length == 0 || self.detailAddrTextField.text.length == 0 || self.startDateLabel.text.length == 0 || self.endDateLabel.text.length == 0) {

        [WKProgressHUD popMessage:@"请完善数据" inView:self.view duration:1 animated:YES];
    }else{
        
        NSDictionary *dic = @{@"snCode":self.snTextField.text,@"merchant":self.merchantTextField.text,@"merchantName":self.merchantNameTextField.text,@"merchantPhone":self.merchantPhoneTextField.text,@"idCard":self.idCardTextField.text,@"bankNumber":self.bankNumberTextField.text,@"bankName":self.bankNameTextField.text,@"bandAddr":self.bandAddrLabel.text,@"bankInternet":self.bankInternetLabel.text,@"phone":self.phoneTextField.text,@"posAddr":self.posAddrLabel.text,@"detailAddr":self.detailAddrTextField.text,@"startDate":self.startDateLabel.text,@"endDate":self.endDateLabel.text};
        UpdateCertificateViewController *vc = [[UpdateCertificateViewController alloc] init];
        vc.parameters = dic;
        [self.navigationController pushViewController:vc animated:YES];
        
    }
    
    
    
}


#pragma mark - Picker View Data source
-(NSInteger)numberOfComponentsInPickerView:(UIPickerView *)pickerView{
    return 1;
}
-(NSInteger)pickerView:(UIPickerView *)pickerView
numberOfRowsInComponent:(NSInteger)component{
    return [pickerArray count];
}

#pragma mark- Picker View Delegate

-(void)pickerView:(UIPickerView *)pickerView didSelectRow:
(NSInteger)row inComponent:(NSInteger)component{
    [textField setText:[pickerArray objectAtIndex:row]];
    myPickerView.hidden = YES;
}
- (NSString *)pickerView:(UIPickerView *)pickerView titleForRow:
(NSInteger)row forComponent:(NSInteger)component{
    return [pickerArray objectAtIndex:row];
}

#pragma mark UITableViewDelegate

-(NSInteger)numberOfSectionsInTableView:(UITableView *)tableView{
    
    return 1;
}

