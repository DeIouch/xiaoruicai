@interface HomePageActivateMachineViewController ()<UITableViewDelegate,UITableViewDataSource,UITextFieldDelegate,UIPickerViewDataSource,UIPickerViewDelegate>
{
    UIPickerView *myPickerView;
    NSMutableArray *pickerArray;
    UITextField *textField;
}

@property(nonatomic, strong) UITableView *tableView;

@property(nonatomic, strong) UITextField *snTextField;
@property(nonatomic, strong) UITextField *merchantTextField;
@property(nonatomic, strong) UITextField *merchantNameTextField;
@property(nonatomic, strong) UITextField *merchantPhoneTextField;
@property(nonatomic, strong) UITextField *idCardTextField;
@property(nonatomic, strong) UITextField *bankNumberTextField;
@property(nonatomic, strong) UITextField *bankNameTextField;
@property(nonatomic, strong) UILabel *bandAddrLabel;
@property(nonatomic, strong) UILabel *bankInternetLabel;
@property(nonatomic, strong) UITextField *phoneTextField;
